{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "right",
			"overflow": "hide",
			"segments": [
				{
					"foreground": "#a9ffb4",
					"properties": {
						"style": "dallas",
						"threshold": 0
					},
					"style": "powerline",
					"template": " {{ .FormattedMs }}s <#ffffff></>",
					"type": "executiontime"
				},
				{
					"foreground": "#45bf17",
					"style": "powerline",
					"template": "  {{ .Full }} ",
					"type": "node"
				},
				{
					"foreground": "#FE4A49",
					"style": "powerline",
					"template": "<#F3EFF5>and</>  {{ .Full }} ",
					"type": "npm"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": true,
			"overflow": "break",
			"segments": [
				{
					"foreground": "#ffafd2",
					"properties": {
						"folder_icon": "",
						"folder_separator_icon": " ❯ ",
						"home_icon": "home",
						"style": "agnoster_full"
					},
					"style": "powerline",
					"template": " {{ .Path }} ",
					"type": "path"
				},
				{
					"foreground": "#f14e32",
					"properties": {
						"branch_icon": " "
					},
					"style": "powerline",
					"template": "({{ .HEAD }})",
					"type": "git"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "#00c7fc",
					"properties": {
						"always_enabled": true
					},
					"style": "diamond",
					"template": "<#00c7fc>❯</>_: ",
					"type": "exit"
				}
			],
			"type": "prompt"
		}
	],
	"console_title_template": "{{ .Folder }}",
	"transient_prompt": {
		"background": "transparent",
		"foreground": "#FEF5ED",
		"template": "{{ .Shell }}"
	},
	"version": 2
}
