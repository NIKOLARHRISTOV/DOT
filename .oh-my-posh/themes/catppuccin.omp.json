{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"segments": [
				{
					"background": "p:blue",
					"foreground": "p:wight",
					"leading_diamond": "",
					"powerline_symbol": "",
					"style": "diamond",
					"template": "{{.Icon}} ",
					"type": "os"
				},
				{
					"background": "p:blue",
					"foreground": "p:text",
					"powerline_symbol": "",
					"style": "diamond",
					"template": "{{ .UserName }}@{{ .HostName }}",
					"type": "session"
				},
				{
					"background": "p:pink",
					"foreground": "p:text",
					"powerline_symbol": "",
					"properties": {
						"folder_icon": "....",
						"home_icon": "~",
						"style": "agnoster_short"
					},
					"style": "powerline",
					"template": " {{ .Path }}",
					"type": "path"
				},
				{
					"background": "p:lavender",
					"foreground": "p:text",
					"powerline_symbol": "",
					"properties": {
						"branch_icon": " ",
						"cherry_pick_icon": " ",
						"commit_icon": " ",
						"fetch_status": false,
						"fetch_upstream_icon": false,
						"merge_icon": " ",
						"no_commits_icon": " ",
						"rebase_icon": " ",
						"revert_icon": " ",
						"tag_icon": " "
					},
					"style": "powerline",
					"template": " {{ .HEAD }}",
					"type": "git"
				}
			],
			"type": "prompt"
		}
	],
	"final_space": true,
	"palette": {
		"blue": "#8AADF4",
		"lavender": "#B7BDF8",
		"os": "#ACB0BE",
		"pink": "#F5BDE6",
		"text": "#494D64",
		"wight": "#FFFFFF"
	},
	"version": 2
}
