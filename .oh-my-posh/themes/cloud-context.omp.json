{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"newline": false,
			"segments": [
				{
					"background": "p:background-color",
					"foreground": "p:timer-text",
					"powerline_symbol": "",
					"properties": {
						"time_format": "15:04:05"
					},
					"style": "powerline",
					"template": " <p:symbol-color></> {{ dateInZone .Format .CurrentDate \"UTC\" }} ",
					"type": "time"
				},
				{
					"background": "p:background-color",
					"foreground": "p:kubernetes-text",
					"powerline_symbol": "",
					"style": "powerline",
					"template": "<p:symbol-color>ﴱ</> {{.Context}}{{if .Namespace}}  {{.Namespace}}{{end}}",
					"type": "kubectl"
				},
				{
					"background": "p:background-color",
					"foreground": "p:cloud-text-amazon",
					"powerline_symbol": "",
					"style": "powerline",
					"template": " <p:symbol-color></> {{.Profile}}{{if .Region}}  {{.Region}}{{end}}",
					"type": "aws"
				},
				{
					"background": "p:background-color",
					"foreground": "p:cloud-text-azure",
					"powerline_symbol": "",
					"properties": {
						"source": "cli"
					},
					"style": "powerline",
					"template": " <p:symbol-color>ﴃ</> {{ .Name }}",
					"type": "az"
				},
				{
					"background": "p:background-color",
					"foreground": "p:cloud-text-azure",
					"powerline_symbol": "",
					"properties": {
						"source": "pwsh"
					},
					"style": "powerline",
					"template": " <p:symbol-color>ﴃ</> (PS) {{ .Name }}",
					"type": "az"
				},
				{
					"background": "p:background-color",
					"foreground": "p:cloud-text-gcp",
					"powerline_symbol": "",
					"style": "powerline",
					"template": " <p:symbol-color></> {{ .Project }}",
					"type": "gcp"
				},
				{
					"background": "p:background-color",
					"foreground": "p:git-text",
					"powerline_symbol": "",
					"style": "powerline",
					"template": " <p:symbol-color></> {{ .RepoName }}",
					"type": "git"
				},
				{
					"background": "p:background-color",
					"foreground": "p:timer-text",
					"powerline_symbol": "",
					"properties": {
						"style": "austin",
						"threshold": 1
					},
					"style": "powerline",
					"template": " <p:symbol-color>ﮫ</> {{ .FormattedMs }}",
					"type": "executiontime"
				},
				{
					"background": "p:background-color",
					"foreground": "p:symbol-color",
					"powerline_symbol": "",
					"properties": {
						"always_enabled": true
					},
					"style": "powerline",
					"template": "{{ if eq .Code 0 }}  {{ end }}",
					"type": "exit"
				},
				{
					"background": "p:error-background",
					"foreground": "p:error-text",
					"leading_diamond": " ",
					"style": "diamond",
					"template": "{{ if ne .Code 0 }}  {{ .Code }}{{ if (ne .Meaning (toString .Code)) }} - {{ .Meaning }}{{else}}{{ end }} {{ end }}",
					"trailing_diamond": "",
					"type": "exit"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"background": "p:background-color",
					"foreground": "p:timer-text",
					"powerline_symbol": "",
					"properties": {
						"time_format": "15:04:05"
					},
					"style": "powerline",
					"template": " <p:symbol-color></> {{ .CurrentDate | date .Format }} ",
					"type": "time"
				},
				{
					"background": "p:background-color",
					"foreground": "p:symbol-color",
					"powerline_symbol": "",
					"properties": {
						"folder_separator_icon": "/",
						"style": "folder"
					},
					"style": "powerline",
					"template": " {{ path .Path .Location }} ",
					"type": "path"
				},
				{
					"background": "p:background-color",
					"foreground": "p:git-text",
					"powerline_symbol": "",
					"properties": {
						"fetch_stash_count": true,
						"fetch_status": true
					},
					"style": "powerline",
					"template": "{{ .HEAD }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} ",
					"type": "git"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"background": "p:error-background",
					"foreground": "p:backgrond-color",
					"leading_diamond": "",
					"style": "diamond",
					"template": "  ",
					"trailing_diamond": "",
					"type": "root"
				},
				{
					"background": "p:background-color",
					"foreground": "p:git-text",
					"style": "plain",
					"template": "{{ if .Root }}{{ else }}<p:symbol-color> > </>{{ end }}",
					"type": "text"
				}
			],
			"type": "prompt"
		}
	],
	"console_title_template": "{{ if or .Root .Segments.Git.RepoName }}[ {{ if .Root }}Admin{{ end }}{{ if and .Root .Segments.Git.RepoName }}@{{ end }}{{ if .Segments.Git.RepoName }}{{ .Segments.Git.RepoName }}{{ end }} ]{{ end }} {{ .Folder }}",
	"palette": {
		"background-color": "transparent",
		"cloud-text-amazon": "#4285F4",
		"cloud-text-azure": "#4285F4",
		"cloud-text-gcp": "#4285F4",
		"error-background": "#dd0033",
		"error-text": "#242424",
		"git-text": "#238636",
		"kubernetes-text": "#FFBB00",
		"symbol-color": "#ffffff",
		"timer-text": "#dd0033"
	},
	"version": 2
}
