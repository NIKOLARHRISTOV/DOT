				}
			],
			"type": "prompt"
		},
		{
			"alignment": "right",
			"segments": [
				{
					"background": "#29315A",
					"foreground": "#43CCEA",
					"leading_diamond": "ÓÉÖ",
					"style": "diamond",
					"template": "  {{ .UserName }}<transparent> / </>{{ .HostName }}",
					"type": "session"
				},
				{
					"background": "#29315A",
					"foreground": "#3EC669",
