						"{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
						"{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
						"{{ if gt .Ahead 0 }}#B388FF{{ end }}",
						"{{ if gt .Behind 0 }}#B388FF{{ end }}"
					],
					"foreground": "#000000",
					"powerline_symbol": "ÓÇ∞",
					"properties": {
						"fetch_stash_count": true,
						"fetch_status": true
					},
					"style": "powerline",
					"template": " {{ .HEAD }}{{ if .Staging.Changed }}<#FF6F00> ÔÅÜ {{ .Staging.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} Ôöí {{ .StashCount }}{{ end }} ",
					"type": "git"
				},
				{
					"background": "#49404f",
					"foreground": "#ffffff",
					"leading_diamond": "<transparent,#49404f>ÓÇ∞</>",
					"properties": {
						"style": "dallas",
						"threshold": 0
					},
					"style": "diamond",
					"template": " {{ .FormattedMs }}s ",
					"trailing_diamond": "ÓÇ∞",
					"type": "executiontime"
				},
				{
					"background": "#910000",
