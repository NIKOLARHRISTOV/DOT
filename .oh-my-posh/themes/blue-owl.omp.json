{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"segments": [
				{
					"background": "#a313a8",
					"foreground": "#FFEB3B",
					"style": "plain",
					"template": "‚ö° ",
					"type": "root"
				},
				{
					"background": "transparent",
					"foreground": "#ffffff",
					"style": "plain",
					"template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
					"type": "os"
				},
				{
					"background": "#01579B",
