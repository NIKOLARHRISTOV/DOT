{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"segments": [
				{
					"background": "#E36464",
					"foreground": "#fff",
					"leading_diamond": "",
					"style": "diamond",
					"template": "{{ .UserName }} ",
					"trailing_diamond": "",
					"type": "session"
				},
				{
					"background": "green",
					"foreground": "#fff",
					"leading_diamond": "",
					"properties": {
						"style": "folder"
					},
					"style": "diamond",
					"template": " {{ .Path }} ",
					"trailing_diamond": "",
					"type": "path"
				},
				{
					"background": "lightCyan",
					"foreground": "#fff",
					"leading_diamond": "",
					"properties": {
						"branch_icon": ""
					},
					"style": "diamond",
					"template": " git({{ .HEAD }}) ",
					"trailing_diamond": "",
					"type": "git"
				},
				{
					"background": "lightMagenta",
					"foreground": "#fff",
					"leading_diamond": "",
					"properties": {
						"time_format": "15:04"
					},
					"style": "diamond",
					"template": " {{ .CurrentDate | date .Format }} ",
					"trailing_diamond": "",
					"type": "time"
				}
			],
			"type": "prompt"
		}
	],
	"final_space": true,
	"version": 2
}
