{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"segments": [
				{
					"foreground": "#007ACC",
					"properties": {
						"time_format": "15:04:05"
					},
					"style": "plain",
					"template": " {{ .CurrentDate | date .Format }} ",
					"type": "time"
				}
			],
			"type": "rprompt"
		},
		{
			"alignment": "left",
			"segments": [
				{
					"background": "#ffffff",
					"foreground": "#100e23",
					"powerline_symbol": "",
					"style": "powerline",
					"template": " {{ .UserName }}@{{ .HostName }} ",
					"type": "session"
				},
				{
					"background": "#91ddff",
					"foreground": "#100e23",
					"powerline_symbol": "",
					"properties": {
						"folder_icon": "",
						"folder_separator_icon": "  ",
						"home_icon": "",
						"style": "agnoster"
					},
					"style": "powerline",
					"template": " {{ .Path }} ",
					"type": "path"
				},
				{
					"background": "#95ffa4",
					"foreground": "#193549",
					"powerline_symbol": "",
					"style": "powerline",
					"template": " {{ .HEAD }} ",
					"type": "git"
				}
			],
			"type": "prompt"
		}
	],
	"final_space": true,
	"version": 2
}
