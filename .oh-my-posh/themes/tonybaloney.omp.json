{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"segments": [
				{
					"background": "#18354c",
					"foreground": "#ffc107",
					"leading_diamond": "",
					"properties": {
						"style": "folder"
					},
					"style": "diamond",
					"template": "  {{ .Path }} ",
					"trailing_diamond": "",
					"type": "path"
				},
				{
					"background": "#18354c",
					"foreground": "#ffc107",
					"powerline_symbol": "",
					"properties": {
						"fetch_stash_count": true,
						"fetch_upstream_icon": true
					},
					"style": "powerline",
					"template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} ",
					"type": "git"
				},
				{
					"background": "#ffc107",
					"foreground": "#18354c",
					"powerline_symbol": "",
					"style": "powerline",
					"template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
					"type": "python"
				},
				{
					"background": "#ffc107",
					"foreground": "#18354c",
					"powerline_symbol": "",
					"style": "powerline",
					"template": "  ",
					"type": "root"
				}
			],
			"type": "prompt"
		}
	],
	"final_space": true,
	"version": 2
}
