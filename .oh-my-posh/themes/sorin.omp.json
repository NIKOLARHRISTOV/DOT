{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"segments": [
				{
					"foreground": "#CB4B16",
					"style": "plain",
					"template": " {{ .Meaning }} ",
					"type": "exit"
				},
				{
					"foreground": "#CECE04",
					"style": "plain",
					"template": "  ",
					"type": "root"
				},
				{
					"foreground": "#FFFFFF",
					"style": "plain",
					"template": "{{ .UserName }}@{{ .HostName }} ",
					"type": "session"
				},
				{
					"foreground": "#0973C0",
					"properties": {
						"style": "full"
					},
					"style": "plain",
					"template": "{{ .Path }} ",
					"type": "path"
				},
				{
					"foreground": "#C1C106",
					"style": "plain",
					"template": "<#ffffff>git:</>{{ .HEAD }} ",
					"type": "git"
				},
				{
					"foreground": "#100e23",
					"style": "plain",
					"template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
					"type": "python"
				},
				{
					"foreground": "#ffffff",
					"style": "plain",
					"template": "<#CB4B16>❯</><#CACA02>❯</><#4E9A06>❯</> ",
					"type": "text"
				}
			],
			"type": "prompt"
		}
	],
	"version": 2
}
